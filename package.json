{
  "name": "socket.io-client",
  "version": "2.3.1",
  "keywords": [
    "realtime",
    "framework",
    "websocket",
    "tcp",
    "events",
    "client"
  ],
  "files": [
    "dist/",
    "build/"
  ],
  "type": "commonjs",
  "main": "./build/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./dist/socket.io.js": "./dist/socket.io.js",
    "./dist/socket.io.js.map": "./dist/socket.io.js.map",
    ".": {
      "import": "./wrapper.mjs",
      "require": "./build/index.js"
    }
  },
  "types": "./build/index.d.ts",
  "dependencies": {
    "backo2": "1.0.2",
    "component-bind": "1.0.0",
    "component-emitter": "~1.3.0",
    "debug": "~3.1.0",
    "engine.io-client": "~4.0.0",
    "has-binary2": "~1.0.2",
    "indexof": "0.0.1",
    "parseuri": "0.0.6",
    "socket.io-parser": "github:socketio/socket.io-parser#develop",
    "to-array": "0.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@types/node": "^14.11.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "6.24.1",
    "base64-arraybuffer": "^0.1.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "4.4.0",
    "eslint-plugin-standard": "1.3.1",
    "expect.js": "0.3.1",
    "has-cors": "^1.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.3.0",
    "prettier": "^2.1.2",
    "socket.io": "github:socketio/socket.io#develop",
    "socket.io-browsers": "^1.0.0",
    "text-blob-builder": "0.0.1",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-remove-debug": "^0.1.0",
    "zuul": "~3.11.1",
    "zuul-builder-webpack": "^1.2.0",
    "zuul-ngrok": "4.0.0"
  },
  "scripts": {
    "test": "npm run format:check && tsc && if test \"$BROWSERS\" = \"1\" ; then npm run test:browser; else npm run test:node; fi",
    "test:node": "mocha --reporter dot --require test/support/server.js test/index.js",
    "test:browser": "zuul test/index.js",
    "build": "webpack --config ./support/webpack.config.js --config ./support/prod.config.js",
    "format:check": "prettier --check 'lib/**/*.ts' 'test/**/*.js' 'support/**/*.js'",
    "format:fix": "prettier --write 'lib/**/*.ts' 'test/**/*.js' 'support/**/*.js'"
  },
  "contributors": [
    {
      "name": "Guillermo Rauch",
      "email": "rauchg@gmail.com"
    },
    {
      "name": "Arnout Kazemier",
      "email": "info@3rd-eden.com"
    },
    {
      "name": "Vladimir Dronnikov",
      "email": "dronnikov@gmail.com"
    },
    {
      "name": "Einar Otto Stangvik",
      "email": "einaros@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Automattic/socket.io-client.git"
  },
  "license": "MIT"
}
